class Node:
    def __init__(self,data):
        self.data=data
        self.left=None
        self.right=None

root=Node(1)
root.left=Node(2)
root.right=Node(3)

def preorder(node):
    if node:
        print(node.data,end=" ")
        preorder(node.left)
        preorder(node.right)

print("Preorder:")
preorder(root)

