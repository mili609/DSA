class Node:
    def __init__(self,data):
        self.data=data
        self.left=None
        self.right=None

root=Node(1)
root.left=Node(2)
root.right=Node(3)

def postorder(node):
    if node:
        postorder(node.left)
        postorder(node.right)
        print(node.data,end=" ")

print("Postorder:")
postorder(root)
